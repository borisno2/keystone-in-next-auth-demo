{
	"name": "keystone-in-next-auth-demo",
	"scripts": {
		"build": "keystone build && next build",
		"build:keystone": "keystone build",
		"build:next": "next build",
		"dev": "concurrently \"pnpm dev:keystone\" \"pnpm dev:next\"",
		"dev:keystone": "next dev .keystone/admin --port 4000",
		"dev:next": "next dev",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"postinstall": "keystone postinstall",
		"start:keystone": "next start .keystone/admin",
		"start:next": "next start"
	},
	"dependencies": {
		"@aws-sdk/signature-v4-crt": "^3.267.0",
		"@keystone-6/core": "^4.0.1",
		"@keystone-6/fields-document": "^6.0.0",
		"@preconstruct/next": "^4.0.0",
		"@prisma/client": "4.3.1",
		"@supabase/auth-helpers-nextjs": "^0.5.2",
		"@supabase/auth-helpers-react": "^0.3.1",
		"@supabase/supabase-js": "^2.4.0",
		"aws-crt": "^1.15.9",
		"dotenv-flow": "^3.2.0",
		"graphql": "^16.6.0",
		"graphql-request": "^5.0.0",
		"graphql-yoga": "^3.1.0",
		"next": "^13.1.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"server-only": "^0.0.1"
	},
	"devDependencies": {
		"@types/dotenv-flow": "^3.2.0",
		"@types/node": "^18.11.11",
		"@types/react": "^18.0.9",
		"@types/react-dom": "^18.0.4",
		"concurrently": "^7.3.0",
		"eslint": "8.29.0",
		"eslint-config-next": "13.1.2",
		"prettier": "^2.5.1",
		"typescript": "^4.9.4"
	},
	"packageManager": "pnpm@7.15.0"
}
