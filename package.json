		"@preconstruct/next": "^4.0.0",
		"@prisma/client": "4.3.1",
		"graphql": "^16.6.0",
		"graphql-request": "^5.0.0",
		"graphql-yoga": "^3.1.0",
		"next": "^13.0.6",
		"next-auth": "^4.18.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/node": "^18.11.11",
		"@types/react": "^18.0.9",
		"@types/react-dom": "^18.0.4",
		"concurrently": "^7.3.0",
		"eslint": "8.29.0",
		"eslint-config-next": "13.0.6",
		"prettier": "^2.5.1",
		"typescript": "^4.8.0"
